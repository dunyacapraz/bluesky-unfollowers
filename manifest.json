{
  "manifest_version": 3,
  "name": "Bluesky Unfollow Tool",
  "version": "1.2",
  "permissions": ["storage", "https://bsky.social/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}